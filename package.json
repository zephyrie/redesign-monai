{
    "name": "monai.io",
    "version": "1.0.0",
    "description": "MONAI - Medical Open Network for AI website",
    "main": "index.js",
    "scripts": {
        "dev": "npm-run-all --parallel watch:* serve",
        "serve": "node scripts/dev-server.js",
        "watch:tailwind": "npx tailwindcss -i ./assets/css/style.css -o ./assets/css/tailwind.css --watch --poll",
        "watch:html": "node scripts/build-html.js --watch",
        "build": "cross-env NODE_ENV=production npm-run-all clean build:*",
        "build:tailwind": "cross-env NODE_ENV=production npx tailwindcss -i ./assets/css/style.css -o ./assets/css/tailwind.css --content './*.html,./components/**/*.html' --minify",
        "build:html": "cross-env NODE_ENV=production node scripts/build-html.js",
        "clean": "rimraf dist",
        "optimize": "cross-env NODE_ENV=production node scripts/optimize-assets.js",
        "start": "cross-env NODE_ENV=production node scripts/dev-server.js"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.7",
        "autoprefixer": "^10.4.7",
        "chokidar": "^3.5.3",
        "connect-livereload": "^0.6.1",
        "cross-env": "^7.0.3",
        "cssnano": "^6.0.1",
        "glob": "^10.3.3",
        "html-minifier": "^4.0.0",
        "live-server": "^1.2.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "rimraf": "^5.0.1",
        "sharp": "^0.32.5",
        "tailwindcss": "^3.1.4",
        "terser": "^5.19.2"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/line-clamp": "^0.4.0",
        "kutty": "^0.6.0"
    }
}
